<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SecondaryController">
    <children>
      <TableView fx:id="fileTable" prefHeight="207.0" prefWidth="588.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="88.0" text="Name" />
          <TableColumn fx:id="fileCol" minWidth="0.0" prefWidth="110.0" text="File" />
            <TableColumn fx:id="pathCol" minWidth="0.0" prefWidth="271.0" text="Path" />
            <TableColumn fx:id="hashCol" minWidth="0.0" prefWidth="118.0" text="Hash" />
        </columns>
      </TableView>
        <Label text="Secondary View" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="uploadBtn" mnemonicParsing="false" onAction="#uploadImage" prefHeight="51.0" prefWidth="122.0" text=" Upload File" />
              <Button fx:id="secondaryButton" onAction="#switchToPrimary" prefHeight="51.0" prefWidth="202.0" text="Switch to Primary View" />
            <Button fx:id="refreshBtn" alignment="CENTER" mnemonicParsing="false" onAction="#initialize" prefHeight="51.0" prefWidth="100.0" text="Refresh">
               <HBox.margin>
                  <Insets left="170.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
