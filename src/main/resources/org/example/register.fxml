<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.RegisterController">
   <children>
      <Text layoutX="224.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="REGISTER">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <TextField fx:id="username_inp" layoutX="226.0" layoutY="138.0" promptText="Username" />
      <TextField fx:id="email_inp" layoutX="226.0" layoutY="188.0" promptText="Email" />
      <PasswordField fx:id="password_inp" layoutX="225.0" layoutY="238.0" promptText="Password" />
      <Button fx:id="register" layoutX="264.0" layoutY="291.0" mnemonicParsing="false" onAction="#register" text="Register">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="30.0" layoutY="27.0" mnemonicParsing="false" onAction="#goBack" text="Voltar" />
   </children>
</AnchorPane>
